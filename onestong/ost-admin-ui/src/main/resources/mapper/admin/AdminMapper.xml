<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.oz.onestong.dao.admin.AdminMapper">


	<select id="selectOneByLoginName" parameterType="map"
		resultType="java.util.HashMap">

		select * from admin a where a.login_name = #{loginName,jdbcType=VARCHAR}

	</select>


	

</mapper>