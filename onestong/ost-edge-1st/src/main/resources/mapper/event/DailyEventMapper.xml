    <select id="selectDailyEventAndCommentsByEventId" parameterType="map"  resultMap="dailyEventAndCommentMap">
  			
SELECT
	de.*, 
	c.comment_id as c_comment_id,
c.user_id AS c_user_id,
	c.creator AS c_creator,
	c.create_time AS c_create_time,
	c.content AS c_content
FROM
	daily_events de
left JOIN comments c ON de.de_id = c.event_id
where 
 de.de_id = #{eventId,jdbcType=CHAR} and de.valid = 0
ORDER BY
	c.update_time DESC
  
  </select>
  
  
  
