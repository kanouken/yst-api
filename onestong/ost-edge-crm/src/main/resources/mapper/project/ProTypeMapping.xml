<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="org.ost.crm.dao.project.ProjectTypeDao">
	<select id="selectTypeCount" resultType="java.lang.Integer">
		SELECT count(1) FROM
		tbl_project_type
	</select>
	<select id="selectTypeList" resultType="org.ost.entity.project.vo.ProjectTypeVo">
		SELECT * FROM
		tbl_project_type
	</select>
	<select id="selectTypeById" resultType="org.ost.entity.project.vo.ProjectTypeVo">
		SELECT * FROM
		tbl_project_type where id=#{id}
	</select>

	<select id="selectByCyc" resultType="org.ost.entity.project.ProjectType">

		SELECT

		cycWarningDay,cycWarningEnable,startTimeWarningDay,startTimeWarningEnable

		FROM

		tbl_project_type

	</select>



	<update id="updateParam">

		UPDATE tbl_project_type

		SET

		cycWarningDay=#{cycWarningDay},cycWarningEnable=#{cycWarningEnable},startTimeWarningDay=#{startTimeWarningDay},startTimeWarningEnable=#{startTimeWarningEnable}

		WHERE id=#{projectTypeID}

	</update>

</mapper>